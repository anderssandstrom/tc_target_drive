
record(bi,"$(P)Dev-EStop-OK"){
  field(PINI, "1")  
  field(DTYP, "asynInt32")
  field(INP,  "@asyn($(PORT),0,1)ADSPORT=852/Main.stHwStatus.bEstopOK?")
  field(SCAN, "I/O Intr")
  field(ZNAM, "NOT OK")
  field(ONAM, "OK")
  field(DESC, "Emergency stop OK")
}

